# HDFS配置
HDFS_NAMESERVICES=haclusterdev
HDFS_NAMENODES=n1,n2
HDFS_RPC_N1=1.hadoopdev.com:8020
HDFS_RPC_N2=2.hadoopdev.com:8020
HDFS_HTTP_N1=1.hadoopdev.com:8090
HDFS_HTTP_N2=2.hadoopdev.com:8090
HDFS_SHARED_EDITS_DIR=qjournal://1.hadoopdev.com:8485;2.hadoopdev.com:8485;3.hadoopdev.com:8485/haclusterdev
HDFS_FAILOVER_PROVIDER=org.apache.hadoop.hdfs.server.namenode.ha.ConfiguredFailoverProxyProvider

# 认证配置
HDFS_USERNAME=hadoop
HDFS_PASSWORD=your_password_here
HDFS_TOKEN=your_token_here

# 开发环境
NODE_ENV=development 